<p id="notice"><%= notice %></p>

<h1>Listing Event Users</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>User</th>
      <th>Role</th>
      <th>Last location</th>
      <th>Location datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_users.each do |event_user| %>
      <tr>
        <td><%= event_user.event_id %></td>
        <td><%= event_user.user_id %></td>
        <td><%= event_user.role %></td>
        <td><%= event_user.last_location %></td>
        <td><%= event_user.location_datetime %></td>
        <td><%= link_to 'Show', event_user %></td>
        <td><%= link_to 'Edit', edit_event_user_path(event_user) %></td>
        <td><%= link_to 'Destroy', event_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event user', new_event_user_path %>
